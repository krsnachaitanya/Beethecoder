{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // "plugin:react/recommended",
    // "standard",
    // "plugin:prettier/recommended"
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": [1],
    "react/prop-types": [0]
  }
}
